{
  "name": "snooplogg",
  "version": "6.0.0",
  "description": "Your mind on your logs and your logs on your mind",
  "type": "module",
  "exports": "./src/index.js",
  "author": "Chris Barber <chris@cb1inc.com> (https://github.com/cb1kenobi)",
  "license": "MIT",
  "keywords": [
    "log",
    "logger",
    "logging",
    "logs",
    "levels",
    "debug",
    "console",
    "snoop",
    "snooping"
  ],
  "scripts": {
    "build": "tsc",
    "check": "biome check --write .",
    "coverage": "vitest --coverage",
    "lint": "biome lint --write .",
    "prepublishOnly": "pnpm run build",
    "test": "vitest",
    "test-no-tty": "node -e \"try { console.log(require('child_process').execSync('pnpm test').toString()); } catch (e) { console.log(e, e.stdout.toString()); console.log(e.stderr.toString()); }\"",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.1",
    "@types/node": "20.14.2",
    "@vitest/coverage-v8": "^1.6.0",
    "lefthook": "1.6.15",
    "typescript": "5.4.5",
    "vitest": "1.6.0"
  },
  "homepage": "https://github.com/cb1kenobi/snooplogg",
  "bugs": "https://github.com/cb1kenobi/snooplogg/issues",
  "repository": "https://github.com/cb1kenobi/snooplogg",
  "engines": {
    "node": ">=18.17"
  }
}
